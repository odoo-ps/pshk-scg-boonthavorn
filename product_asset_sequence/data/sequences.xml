            <field name="number_next" eval="1"/>
            <field name="number_increment" eval="1"/>
        </record>

        <record id="sequence_product_template_consumable" model="ir.sequence">
            <field name="name">Product Template Consumable</field>
            <field name="code">product.template.consumable</field>
            <field name="prefix">PTC</field>
            <field name="number_next" eval="1"/>
